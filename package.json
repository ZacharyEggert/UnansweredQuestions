{
    "name": "unanswered-questions",
    "version": "2.0.0",
    "description": "A project for UW Full Stack Flex Course",
    "main": "server.js",
    "scripts": {
        "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
        "start:prod": "node server.js",
        "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
        "client": "cd client && npm run start",
        "seed": "node scripts/seedDB.js",
        "install": "cd client && npm install",
        "build": "cd client && npm run build",
        "heroku-postbuild": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ZacharyEggert/Unanswered-Questions.git"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ZacharyEggert/Unanswered-Questions/issues"
    },
    "homepage": "https://github.com/ZacharyEggert/Unanswered-Questions#readme",
    "dependencies": {
        "autoprefixer": "^10.2.5",
        "bcrypt": "^5.0.1",
        "connect-session-sequelize": "^7.1.1",
        "cssnano": "^5.0.2",
        "dotenv": "^9.0.2",
        "eslint-config-prettier": "^8.3.0",
        "express": "^4.17.1",
        "express-handlebars": "^5.3.2",
        "express-session": "^1.17.1",
        "if-env": "^1.0.4",
        "moment": "^2.29.1",
        "mysql2": "^2.2.5",
        "node-fetch": "^2.6.1",
        "postcss": "^8.2.15",
        "postcss-cli": "^8.3.1",
        "sequelize": "^6.6.2",
        "socket.io": "^4.0.2",
        "tailwindcss": "^2.1.2",
        "yarn": "^1.22.10",
        "youtube-search": "^1.1.6"
    },
    "devDependencies": {
        "concurrently": "^6.2.0",
        "eslint": "^7.26.0",
        "nodemon": "^2.0.7",
        "prettier": "^2.3.0",
        "yarn": "^1.22.10"
    },
    "engines": {
      "node": "14.16.0",
      "npm": "7.15.0"
    }
}
